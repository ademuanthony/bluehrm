@using SoftBreeze.BlueHrm.Web.Navigation

@{
    ViewBag.CurrentPageName = PageNames.App.Admin.Organisation.GeneralInformation;

    ViewBag.Title = "General Information";
}

<div class="row margin-bottom-5">
    <div class="col-xs-6">
        <div class="page-head">
            <div class="page-title">
                <h1>
                    <span>@L("GeneralInformation")</span> <small>@L("UpdateGeneralInformation")</small>
                </h1>
            </div>
        </div>
    </div>
    <div class="col-xs-6 text-right">
        <button id="SaveAllSettingsButton" class="btn btn-primary blue"><i class="fa fa-floppy-o"></i> @L("SaveAll")</button>
    </div>
</div>

<div class="portlet light">
    <div class="portlet-body">
        <div id="SettingsTabPanel" class="tabbable-line">
            <ul class="nav nav-tabs ">
               
                <li class="active">
                    <a href="#SettingsUserManagementTab" data-toggle="tab" aria-expanded="false">
                        General Information
                    </a>
                </li>
                
            </ul>
            <div class="tab-content">
                
                <div class="tab-pane active" id="SettingsUserManagementTab">
                  
                </div>
               
            </div>
        </div>
    </div>
</div>